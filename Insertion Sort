#include <stdio.h>
void insertionSort(int a[], int n) {
	for(int i = 0; i < n; i++){
		int temp = a[i];
		int j = i - 1;
		while(j >= 0 && a[j] > temp){
			a[j+1] = a[j];
			j--;
		}
		a[j+1] = temp;
	}
}

int main() {
    int a[20], n, i;
    printf("Enter array size: ");
    scanf("%d", &n);
    printf("Enter array elements: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    insertionSort(a, n);

    printf("After sorting: \n");
    for (i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }

    return 0;
}
